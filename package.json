{
  "name": "llama-airforce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "lint:framework": "eslint \"src/{Framework,Services,Wallet,Util}/**/*.{ts,vue}\"",
    "lint:laf": "eslint \"src/Apps/LlamaAirforce/**/*.{ts,vue}\"",
    "lint:cm": "eslint \"src/Apps/CurveMonitor/**/*.{ts,vue}\"",
    "lint:pm": "eslint \"src/Apps/PrismaMonitor/**/*.{ts,vue}\"",
    "lint:pm-lrt": "eslint \"src/Apps/PrismaMonitor/**/*.{ts,vue}\"",
    "lint:cb": "eslint \"src/Apps/Cookbook/**/*.{ts,vue}\"",
    "dev:laf": "bun --bun vite src/Apps/LlamaAirforce",
    "dev:laf:server": "bun run --hot src/Apps/LlamaAirforce/Server/main.ts",
    "dev:cm": "bun --bun vite src/Apps/CurveMonitor",
    "dev:cm:server": "bun run --hot src/Apps/CurveMonitor/Server/main.ts",
    "dev:pm": "bun --bun vite src/Apps/PrismaMonitor",
    "dev:cb": "bun --bun vite src/Apps/Cookbook",
    "build:laf": "bun --bun vite build src/Apps/LlamaAirforce",
    "build:cm": "bun --bun vite build src/Apps/CurveMonitor",
    "build:pm": "bun --bun vite build src/Apps/PrismaMonitor",
    "build:pm-lrt": "bun --bun vite build src/Apps/PrismaMonitor",
    "build:cb": "bun --bun vite build src/Apps/Cookbook",
    "run:laf:server": "bun run src/Apps/LlamaAirforce/Server/main.ts",
    "run:cm:server": "bun run src/Apps/CurveMonitor/Server/main.ts",
    "test": "vitest run --root src/Apps/LlamaAirforce",
    "union": "bun run src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.ts",
    "union:allocate": "bun src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.allocate.ts",
    "union:migrate": "bun run src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.migrate.ts",
    "deploy": "bun run Deploy.ts"
  },
  "dependencies": {
    "@cowprotocol/widget-lib": "0.16.0",
    "@kyvg/vue3-notification": "^3.4.1",
    "@tanstack/vue-query": "5.59.20",
    "@vueuse/core": "11.2.0",
    "@vueuse/router": "11.2.0",
    "@wagmi/vue": "0.0.60",
    "apexcharts": "4.0.0",
    "ethereum-blockies-base64": "^1.0.2",
    "hono": "4.6.9",
    "lightweight-charts": "4.2.1",
    "lru-cache": "^11.0.2",
    "lucide-vue-next": "^0.455.0",
    "pinia": "2.2.6",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.8.1",
    "viem": "2.21.43",
    "vue": "3.5.12",
    "vue-i18n": "10.0.4",
    "vue-router": "4.4.5",
    "vue3-apexcharts": "^1.7.0"
  },
  "devDependencies": {
    "@azure/cosmos": "4.1.1",
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@safe-global/api-kit": "2.5.4",
    "@safe-global/protocol-kit": "5.0.4",
    "@safe-global/safe-core-sdk-types": "5.1.0",
    "@tanstack/vue-query-devtools": "5.59.20",
    "@vitejs/plugin-vue": "5.1.4",
    "consola": "^3.2.3",
    "eslint": "9.14.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-vue": "9.30.0",
    "shiki": "1.22.2",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.13.0",
    "unplugin-auto-import": "0.18.3",
    "unplugin-vue-components": "0.27.4",
    "vite": "5.4.10",
    "vite-plugin-vue-devtools": "7.6.3",
    "vitest": "2.1.4",
    "vue-tsc": "2.1.10"
  }
}
