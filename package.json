{
  "name": "llama-airforce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "lint:laf": "eslint \"src/Apps/LlamaAirforce/**/*.{ts,vue}\"",
    "lint:cm": "eslint \"src/Apps/CurveMonitor/**/*.{ts,vue}\"",
    "lint:pm": "eslint \"src/Apps/PrismaMonitor/**/*.{ts,vue}\"",
    "lint:cb": "eslint \"src/Apps/Cookbook/**/*.{ts,vue}\"",
    "dev:laf": "bun --bun vite src/Apps/LlamaAirforce",
    "dev:laf:server": "bun run --hot src/Apps/LlamaAirforce/Server/main.ts",
    "dev:cm": "bun --bun vite src/Apps/CurveMonitor",
    "dev:cm:server": "bun run --hot src/Apps/CurveMonitor/Server/main.ts",
    "dev:pm": "bun --bun vite src/Apps/PrismaMonitor",
    "dev:cb": "bun --bun vite src/Apps/Cookbook",
