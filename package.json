{
  "name": "llama-airforce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "lint:laf": "eslint \"src/Apps/LlamaAirforce/**/*.{ts,vue}\"",
    "lint:cm": "eslint \"src/Apps/CurveMonitor/**/*.{ts,vue}\"",
    "lint:pm": "eslint \"src/Apps/PrismaMonitor/**/*.{ts,vue}\"",
    "lint:cb": "eslint \"src/Apps/Cookbook/**/*.{ts,vue}\"",
    "dev:laf": "bun --bun vite src/Apps/LlamaAirforce",
    "dev:laf:server": "bun --bun nitro dev src/Apps/LlamaAirforce --host --port 3000",
    "dev:cm": "bun --bun vite src/Apps/CurveMonitor",
    "dev:cm:server": "bun --bun nitro dev src/Apps/CurveMonitor --host --port 3001",
    "dev:pm": "bun --bun vite src/Apps/PrismaMonitor",
    "dev:cb": "bun --bun vite src/Apps/Cookbook",
    "build:laf": "bun --bun vite build src/Apps/LlamaAirforce",
    "build:laf:server": "bun --bun nitro build --preset bun src/Apps/LlamaAirforce",
    "build:cm": "bun --bun vite build src/Apps/CurveMonitor",
    "build:cm:server": "bun --bun nitro build --preset bun src/Apps/CurveMonitor",
    "build:pm": "bun --bun vite build src/Apps/PrismaMonitor",
    "build:pm-lrt": "bun --bun vite build src/Apps/PrismaMonitor",
    "build:cb": "bun --bun vite build src/Apps/Cookbook",
    "run:laf:server": "NITRO_PORT=3000 bun --bun --smol src/Apps/LlamaAirforce/.output/server/index.mjs",
    "run:cm:server": "NITRO_PORT=3001 bun --bun --smol src/Apps/CurveMonitor/.output/server/index.mjs",
    "test": "vitest run --root src/Apps/LlamaAirforce",
    "union": "bun run src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.ts",
    "union:allocate": "bun src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.allocate.ts",
    "union:migrate": "bun run src/Apps/LlamaAirforce/Pages/Union/Scripts/Union.migrate.ts",
    "deploy": "bun run Deploy.ts"
  },
  "dependencies": {
    "@cowprotocol/widget-lib": "0.13.2",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@kyvg/vue3-notification": "^3.2.1",
    "@tanstack/vue-query": "5.51.9",
    "@tanstack/vue-query-devtools": "5.51.9",
    "@vueuse/core": "^10.11.0",
    "@vueuse/router": "^10.11.0",
    "@wagmi/vue": "0.0.31",
    "apexcharts": "3.51.0",
    "lightweight-charts": "4.1.7",
    "lodash": "^4.17.21",
    "nitropack": "2.9.7",
    "pinia": "^2.1.7",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.7.5",
    "viem": "2.17.11",
    "vue": "3.4.33",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0",
    "vue3-apexcharts": "^1.5.2",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@azure/cosmos": "^4.0.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@safe-global/api-kit": "2.4.3",
    "@safe-global/protocol-kit": "4.0.3",
    "@safe-global/safe-core-sdk-types": "5.0.3",
    "@types/lodash": "4.17.7",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitejs/plugin-vue": "5.1.0",
    "@vue/compiler-sfc": "3.4.33",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "consola": "^3.2.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-vue": "9.27.0",
    "sass": "1.77.8",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "unplugin-auto-import": "0.18.1",
    "unplugin-vue-components": "0.27.3",
    "vite": "5.3.4",
    "vite-plugin-vue-devtools": "7.3.7",
    "vitest": "2.0.4",
    "vue-tsc": "2.0.28"
  }
}
