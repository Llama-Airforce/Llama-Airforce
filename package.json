    "@vueuse/router": "11.1.0",
    "@wagmi/vue": "0.0.46",
    "apexcharts": "3.53.0",
    "hono": "4.6.3",
    "lightweight-charts": "4.2.0",
    "lru-cache": "^11.0.1",
    "pinia": "2.2.2",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.8.0",
    "viem": "2.21.14",
    "vue": "3.5.8",
    "vue-i18n": "10.0.3",
    "vue-router": "4.4.5",
    "vue3-apexcharts": "^1.6.0"
  },
  "devDependencies": {
    "@azure/cosmos": "4.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
